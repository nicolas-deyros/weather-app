{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/ping\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/simple\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/test\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/weather-simple\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/weather\\.json/?$",
			"dest": "_render"
		}
	]
}
